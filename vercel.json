{
    "version": 2,
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHONIOENCODING": "utf-8",
        "LANG": "zh_CN.UTF-8"
    }
} 